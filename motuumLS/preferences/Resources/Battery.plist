				<string>kMLSBatteryTipEnabled</string>
				<key>default</key>
				<true/>
				<key>PostNotification</key>
				<string>com.creaturesurvive.motuumls.prefschanged</string>
				<key>defaults</key>
				<string>com.creaturesurvive.motuumls</string>
