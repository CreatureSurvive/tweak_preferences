<?xml version=1.0 encoding=UTF-8?>
<!DOCTYPE plist PUBLIC -//Apple//DTD PLIST 1.0//EN http://www.apple.com/DTDs/PropertyList-1.0.dtd>
<plist version=1.0>
	<dict>
		<key>items</key>
		<array>

			<dict>
				<key>action</key>
				<string>pushToView:</string>
				<key>headerString</key>
				<string>Battery</string>
				<key>headerFont</key>
				<string>.SFUIDisplay-Regular</string>
				<key>primaryColor</key>
				<string>#5eb5d8</string>
				<key>primaryFontSize</key>
				<real>30</real>
				<key>cell</key>
				<string>PSLinkCell</string>
				<key>pushToPlist</key>
				<string>:batteryOptionsState</string>
				<key>default</key>
				<string>BatteryState0</string>
				<key>copyToTitlesAndKeys</key>
				<dict>
					<key>Default</key>
					<string>0</string>
					<key>Default With Notifications</key>
					<string>1</string>
					<key>Now Playing</key>
					<string>2</string>
					<key>Now Playing With Notifications</key>
					<string>3</string>
					<key>Today</key>
					<string>4</string>
					<key>Copy To All</key>
					<string>-1</string>
				</dict>
				<key>label</key>
				<string>Battery States</string>
			</dict>

			<dict>
				<key>cell</key>
				<string>PSGroupCell</string>
				<key>label</key>
				<string>Battery Colors</string>
			</dict>

			<dict>
				<key>cell</key>
				<string>PSLinkCell</string>
				<key>cellClass</key>
				<string>CSColorDisplayCell</string>
				<key>defaultsPath</key>
				<string>/Library/PreferenceBundles/motuumls.bundle</string>
